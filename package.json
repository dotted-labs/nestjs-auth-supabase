{
  "name": "@dotted-labs/nestjs-supabase-auth",
  "version": "0.0.2",
  "description": "NestJS library for Supabase authentication",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepublish": "npm run build"
  },
  "repository": "https://github.com/dotted-labs/nestjs-supabase-auth",
  "keywords": [
    "nestjs",
    "supabase",
    "authentication",
    "auth"
  ],
  "author": "DottedLabs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotted-labs/nestjs-supabase-authissues"
  },
  "homepage": "https://github.com/dotted-labs/nestjs-supabase-auth#readme",
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@nestjs/common": "^11.0.14",
    "@nestjs/core": "^11.0.14",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/passport-jwt": "^4.0.1",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@nestjs/testing": "11.0.14",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.14.0",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "eslint": "9.24.0",
    "jest": "29.7.0",
    "ts-jest": "29.3.1",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2"
  }
}
